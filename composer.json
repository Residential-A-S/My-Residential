{
  "name": "residential/residential",
  "description": "Residential is a property management system",
  "license": "AGPL-3.0-or-later",
  "version": "0.1.0",
  "autoload": {
    "psr-4": {
      "Adapter\\": "src/Adapter/",
      "Application\\": "src/Application/",
      "Domain\\": "src/Domain/",
      "Shared\\": "src/Shared/"
    },
    "files": [
      "config.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/"
    }
  },
  "require": {
    "ext-curl": "*",
    "ext-pdo": "*",
    "php": ">=8.4",
    "twig/twig": "v3.21.1",
    "phpmailer/phpmailer": "^6.10"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "scripts": {
    "phpcs": "phpcs --standard=phpcs.xml"
  }
}
